        "./front/app/src/controllers/newsfeed/newsfeed.ts",
        "./front/app/src/directives/loggedin.ts",
        "./front/app/src/services/api.ts",
        "./front/app/src/services/api/client.ts",
        "./front/app/src/services/cookie.ts",
        "./front/app/src/services/events.ts",
        "./front/app/src/services/navigation.ts",
        "./front/app/src/services/session.ts",
        "./front/app/src/services/storage.ts",
        "./front/app/src/services/ui.ts",
        "./front/app/src/services/ui/loader.ts",
        "./front/app/src/services/ui/material.ts",
        "./front/app/src/services/ui/sidebar.ts",
        "./front/typings/angular2/angular2.d.ts",
        "./front/typings/angular2/router.d.ts",
        "./front/typings/es6-promise/es6-promise.d.ts",
        "./front/typings/minds/minds.d.ts",
        "./front/typings/rx/rx-lite.d.ts",
        "./front/typings/rx/rx.d.ts",
        "./front/typings/tsd.d.ts"
    ]
